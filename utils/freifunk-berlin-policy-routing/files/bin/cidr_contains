#!/bin/sh

range=$1
subrange=$2
eval $(/bin/ipcalc.sh $range)
range_net=$NETWORK
range_prefix=$PREFIX

subrange_addr=$(echo $subrange | cut -d / -f 1)
eval $(/bin/ipcalc.sh $subrange_addr/$range_prefix)
subrange_net=$NETWORK

if [ $range_net = $subnet_net ]; then
  echo true
else
  echo false
  exit 1
fi
