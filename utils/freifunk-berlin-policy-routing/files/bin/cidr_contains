#!/bin/sh

range=$1      # 10.10.10.1/24
subrange=$2   # 10.10.10.20/30

eval $(/bin/ipcalc.sh $range)
range_net=$NETWORK       # 10.10.10.0
range_prefix=$PREFIX     # 24

subrange_addr=$(echo $subrange | cut -d / -f 1)       # 10.10.10.20
eval $(/bin/ipcalc.sh $subrange_addr/$range_prefix)
subrange_net=$NETWORK    # 10.10.10.0              
                                                   
if [ $range_net = $subrange_net ]; then
  echo true                            
else                         
  echo false                           
  exit 1                               
fi                                     
